<h1>Welcome to Dumb Meetup!</h1>

<%= form_for @user do |f| %>
  <label for="user_name">Name</label>
  <%= f.text_field :name %>
  <br>

  <label for="user_email">Email</label>
  <%= f.text_field :email %>
  <br>

  <label for="user_password">Password</label>
  <%= f.password_field :password %>
  <br>

  <%= f.submit "Create" %>
<% end %>